{
    "manifest_version": 2,
    "name": "Social Media Blocker - Chrome Ext.",
    "version": "0.1.1",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["internal.js"]
      }
    ],
    "permissions": [
      "https://www.youtube.com/*",
      "https://www.facebook.com/*",
      "https://www.netflix.com/*"
    ],
    "icons": {
        "16": "icons/logo_16x16.png",
        "48": "icons/logo_48x48.png",
        "128": "icons/logo_128x128.png"
    }
  }